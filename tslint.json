{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-config-airbnb",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "import-name": [false, {
        "react": "React",
        "react-dom": "ReactDOM"
      }],
      "object-curly-spacing": ["warning", "never"],
      "max-line-length": [true, 140]
    },
    "rulesDirectory": []
}