{{#is_list}}
    [
        {{#list_type}}{{> from_json}}{{/list_type}} for tmp in (tmp or [])
    ]
{{/is_list}}
{{#is_map}}
    {
        k: {{#map_val_type}}{{> from_json}}{{/map_val_type}}
        for k, tmp
        in tmp.items()
    }
{{/is_map}}
{{#is_object}}
    {
        {{#properties}}
            "{{key}}": (lambda tmp: {{#map_val_type}}{{> from_json}}{{/map_val_type}})
                       (tmp.get("{{key}}")),
        {{/properties}}
    }
{{/is_object}}
{{#is_basic}}
    tmp
{{/is_basic}}
{{#is_custom}}
    {{#custom}}{{name}}.from_json(tmp){{/custom}}
{{/is_custom}}
